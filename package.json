{
	"name": "@chieforz/leberkas-vote",
	"version": "1.0.0",
	"description": "",
	"keywords": [],
	"license": "MIT",
	"author": "ChiefORZ <seb.schaffernak@gmail.com>",
	"scripts": {
		"build": "wireit",
		"dev": "wireit",
		"format:check": "wireit",
		"format:write": "wireit",
		"generate": "wireit",
		"generate:nexus": "wireit",
		"generate:prisma": "wireit",
		"postinstall": "wireit",
		"lint": "wireit",
		"migrate:deploy": "wireit",
		"migrate:dev": "wireit",
		"migrate:status": "wireit",
		"precommit": "wireit",
		"start": "wireit",
		"test": "wireit",
		"test:knip": "wireit",
		"test:unit": "wireit"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "biome check --apply"
	},
	"eslintConfig": {
		"extends": "@chieforz/eslint-config-tslint-react"
	},
	"dependencies": {
		"@apollo/server": "^4.10.4",
		"@as-integrations/next": "^3.0.0",
		"@aws-sdk/abort-controller": "^3.374.0",
		"@aws-sdk/client-s3": "^3.577.0",
		"@aws-sdk/signature-v4-crt": "^3.577.0",
		"@formkit/auto-animate": "^1.0.0-pre-alpha.3",
		"@headlessui/react": "^1.7.19",
		"@heroicons/react": "^2.1.3",
		"@magic-sdk/admin": "^2.4.1",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^5.14.0",
		"@tailwindcss/forms": "^0.5.7",
		"autoprefixer": "^10.4.19",
		"aws-crt": "^1.21.2",
		"aws-sdk": "^2.1623.0",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0",
		"graphql-scalars": "^1.23.0",
		"magic-sdk": "^28.0.3",
		"micro-cors": "^0.1.1",
		"next": "^13.4.7",
		"next-auth": "^4.22.1",
		"next-s3-upload": "^0.3.4",
		"nexus": "^1.3.0",
		"plaiceholder": "^3.0.0",
		"postcss": "^8.4.38",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.51.4",
		"react-hot-toast": "^2.4.1",
		"sass": "^1.77.2",
		"sharp": "^0.33.4",
		"tailwind-merge": "^2.3.0",
		"tailwindcss": "^3.4.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@chieforz/biome-config": "^1.2.6",
		"@chieforz/eslint-config-tslint-react": "^1.2.6",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^14.3.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/micro-cors": "^0.1.5",
		"@types/node": "^20.12.12",
		"@types/react": "^18.3.2",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@vitejs/plugin-react": "^4.2.1",
		"dotenv": "^16.4.5",
		"encoding": "^0.1.13",
		"eslint": "^9.3.0",
		"husky": "^9.0.11",
		"is-ci": "^3.0.1",
		"jsdom": "^24.0.0",
		"knip": "^5.16.0",
		"lint-staged": "^15.2.2",
		"npm-run-all2": "^6.2.0",
		"prisma": "^5.14.0",
		"resize-observer-polyfill": "^1.5.1",
		"tsx": "^4.10.5",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0",
		"webpack": "^5.91.0",
		"wireit": "^0.14.4"
	},
	"packageManager": "pnpm@8.12.0",
	"engines": {
		"node": "^20"
	},
	"pnpm": {
		"overrides": {
			"@typescript-eslint/eslint-plugin": "6.21.0",
			"@typescript-eslint/parser": "6.21.0",
			"@typescript-eslint/utils": "6.21.0"
		}
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"wireit": {
		"build": {
			"command": "next build",
			"dependencies": ["generate"],
			"files": ["prisma/schema.prisma", "src/**/*", "tsconfig.json"],
			"output": [".next/**"]
		},
		"dev": {
			"command": "next",
			"dependencies": ["generate"],
			"files": ["src/**/*"],
			"output": []
		},
		"format:check": {
			"command": "biome check ./src",
			"files": ["src/**/*.{js,jsx,ts,tsx}"],
			"output": []
		},
		"format:write": {
			"command": "biome check --apply ./src"
		},
		"generate": {
			"command": "run-s generate:prisma generate:nexus"
		},
		"generate:nexus": {
			"command": "tsx --tsconfig nexus.tsconfig.json ./src/pages/api"
		},
		"generate:prisma": {
			"command": "prisma generate"
		},
		"lint": {
			"command": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
			"files": ["src/**/*"],
			"output": []
		},
		"migrate:dev": {
			"command": "is-ci || prisma migrate dev",
			"files": ["prisma/migrations/**/*", "prisma/schema.prisma"],
			"output": ["node_modules/.prisma/client/**"]
		},
		"migrate:deploy": {
			"command": "is-ci || prisma migrate deploy",
			"files": ["prisma/migrations/**/*", "prisma/schema.prisma"],
			"output": ["node_modules/.prisma/client/**"]
		},
		"migrate:status": {
			"command": "is-ci || prisma migrate status",
			"files": ["prisma/migrations/**/*", "prisma/schema.prisma"],
			"output": []
		},
		"start": {
			"command": "next start",
			"dependencies": ["generate:prisma", "migrate:deploy"],
			"files": [".next/**"],
			"output": []
		},
		"test": {
			"command": "npm-run-all test:*",
			"files": ["src/**/*"],
			"output": []
		},
		"test:knip": {
			"command": "knip"
		},
		"test:unit": {
			"command": "vitest --run --passWithNoTests",
			"files": ["src/**/*"],
			"output": []
		},
		"postinstall": {
			"command": "is-ci || husky install"
		},
		"precommit": {
			"command": "lint-staged"
		}
	}
}
