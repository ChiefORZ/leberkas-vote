{
  "name": "@chieforz/leberkas-vote",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "ChiefORZ <seb.schaffernak@gmail.com>",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "generate": "wireit",
    "generate:nexus": "wireit",
    "generate:prisma": "wireit",
    "lint": "wireit",
    "migrate:dev": "wireit",
    "migrate:deploy": "wireit",
    "migrate:status": "wireit",
    "start": "wireit",
    "test": "wireit",
    "test:knip": "wireit",
    "test:unit": "wireit",
    "postinstall": "wireit",
    "precommit": "wireit"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "prettier": "@chieforz/prettier-config",
  "eslintConfig": {
    "extends": "@chieforz/eslint-config-tslint-react"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@as-integrations/next": "^2.0.2",
    "@aws-sdk/abort-controller": "^3.370.0",
    "@aws-sdk/client-s3": "^3.438.0",
    "@aws-sdk/signature-v4-crt": "^3.438.0",
    "@formkit/auto-animate": "^1.0.0-pre-alpha.3",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@magic-sdk/admin": "^2.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "@splitbee/web": "^0.3.0",
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.16",
    "aws-crt": "^1.18.3",
    "aws-sdk": "^2.1483.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-scalars": "^1.22.4",
    "magic-sdk": "^21.2.0",
    "micro-cors": "^0.1.1",
    "next": "^13.4.7",
    "next-auth": "^4.22.1",
    "next-s3-upload": "^0.3.4",
    "nexus": "^1.3.0",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.69.5",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@chieforz/eslint-config-tslint-react": "^1.1.10",
    "@chieforz/prettier-config": "^1.1.10",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/micro-cors": "^0.1.4",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.1.0",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^22.1.0",
    "knip": "^2.38.4",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "resize-observer-polyfill": "^1.5.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0",
    "wireit": "^0.14.1"
  },
  "engines": {
    "node": ">=18.17"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "6.2.0",
      "@typescript-eslint/parser": "6.2.0",
      "@typescript-eslint/utils": "6.2.0"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "wireit": {
    "build": {
      "command": "next build",
      "dependencies": [
        "generate",
        "migrate:deploy"
      ],
      "files": [
        "prisma/schema.prisma",
        "src/**/*",
        "tsconfig.json"
      ],
      "output": [
        ".next/**"
      ]
    },
    "dev": {
      "command": "next",
      "dependencies": [
        "generate"
      ],
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "generate": {
      "command": "run-s generate:prisma generate:nexus",
      "files": [
        "prisma/schema.prisma",
        "src/**/*"
      ],
      "output": []
    },
    "generate:nexus": {
      "command": "tsx --tsconfig nexus.tsconfig.json ./src/pages/api",
      "files": [
        "src/pages/api/**/*",
        "nexus.tsconfig.json"
      ],
      "output": [],
      "[the script command was]": "tsx --tsconfig nexus.tsconfig.json ./src/pages/api"
    },
    "generate:prisma": {
      "command": "prisma generate",
      "files": [
        "prisma/schema.prisma"
      ],
      "output": [
        "node_modules/.prisma/client/**"
      ]
    },
    "lint": {
      "command": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "migrate:dev": {
      "command": "is-ci || prisma migrate dev",
      "files": [
        "prisma/migrations/**/*",
        "prisma/schema.prisma"
      ],
      "output": [
        "node_modules/.prisma/client/**"
      ]
    },
    "migrate:deploy": {
      "command": "is-ci || prisma migrate deploy",
      "files": [
        "prisma/migrations/**/*",
        "prisma/schema.prisma"
      ],
      "output": [
        "node_modules/.prisma/client/**"
      ]
    },
    "migrate:status": {
      "command": "is-ci || prisma migrate status",
      "files": [
        "prisma/migrations/**/*",
        "prisma/schema.prisma"
      ],
      "output": []
    },
    "start": {
      "command": "next start",
      "dependencies": [
        "generate:prisma"
      ],
      "files": [
        ".next/**"
      ],
      "output": []
    },
    "test": {
      "command": "npm-run-all test:*",
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "test:knip": {
      "command": "knip"
    },
    "test:unit": {
      "command": "vitest --run --passWithNoTests",
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "postinstall": {
      "command": "is-ci || husky install"
    },
    "precommit": {
      "command": "lint-staged"
    }
  }
}