{
  "name": "@chieforz/leberkas-vote",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "ChiefORZ <seb.schaffernak@gmail.com>",
  "scripts": {
    "biome:check": "wireit",
    "biome:write": "wireit",
    "build": "wireit",
    "dev": "wireit",
    "generate": "wireit",
    "generate:nexus": "wireit",
    "generate:prisma": "wireit",
    "postinstall": "wireit",
    "lint": "wireit",
    "migrate:deploy": "wireit",
    "migrate:dev": "wireit",
    "migrate:status": "wireit",
    "precommit": "wireit",
    "start": "wireit",
    "test": "wireit",
    "test:knip": "wireit",
    "test:unit": "wireit"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "biome check --write"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@as-integrations/next": "^3.2.0",
    "@aws-sdk/client-s3": "^3.722.0",
    "@aws-sdk/signature-v4-crt": "^3.721.0",
    "@formkit/auto-animate": "^1.0.0-pre-alpha.3",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@magic-sdk/admin": "^2.4.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@tailwindcss/forms": "^0.5.9",
    "autoprefixer": "^10.4.20",
    "aws-crt": "^1.24.2",
    "aws-sdk": "^2.1692.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "graphql-scalars": "^1.24.0",
    "magic-sdk": "^28.20.0",
    "micro-cors": "^0.1.1",
    "next": "^15.0.1",
    "next-auth": "^4.24.10",
    "next-s3-upload": "^0.3.4",
    "nexus": "^1.3.0",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "sass": "^1.83.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/micro-cors": "^0.1.5",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "encoding": "^0.1.13",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-compiler": "19.0.0-beta-df7b47d-20241124",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "jsdom": "^25.0.1",
    "knip": "^5.41.1",
    "lint-staged": "^15.3.0",
    "npm-run-all2": "^7.0.2",
    "prisma": "^5.22.0",
    "resize-observer-polyfill": "^1.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "wireit": "^0.14.9"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": "^20"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "wireit": {
    "build": {
      "command": "next build",
      "dependencies": ["generate"],
      "files": ["prisma/schema.prisma", "src/**/*", "tsconfig.json"],
      "output": [".next/**"]
    },
    "dev": {
      "command": "next --turbo",
      "dependencies": ["generate"],
      "files": ["src/**/*"],
      "output": []
    },
    "biome:check": {
      "command": "biome check ./src",
      "files": ["src/**/*.{js,jsx,ts,tsx}"],
      "output": []
    },
    "biome:write": {
      "command": "biome check --write ./src"
    },
    "generate": {
      "command": "run-s generate:prisma generate:nexus"
    },
    "generate:nexus": {
      "command": "tsx --tsconfig nexus.tsconfig.json ./src/pages/api"
    },
    "generate:prisma": {
      "command": "prisma generate"
    },
    "lint": {
      "command": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
      "files": ["src/**/*"],
      "output": []
    },
    "migrate:dev": {
      "command": "is-ci || prisma migrate dev",
      "files": ["prisma/migrations/**/*", "prisma/schema.prisma"],
      "output": ["node_modules/.prisma/client/**"]
    },
    "migrate:deploy": {
      "command": "is-ci || prisma migrate deploy",
      "files": ["prisma/migrations/**/*", "prisma/schema.prisma"],
      "output": ["node_modules/.prisma/client/**"]
    },
    "migrate:status": {
      "command": "is-ci || prisma migrate status",
      "files": ["prisma/migrations/**/*", "prisma/schema.prisma"],
      "output": []
    },
    "start": {
      "command": "next start",
      "dependencies": ["generate:prisma", "migrate:deploy"],
      "files": [".next/**"],
      "output": []
    },
    "test": {
      "command": "npm-run-all test:*",
      "files": ["src/**/*"],
      "output": []
    },
    "test:knip": {
      "command": "knip"
    },
    "test:unit": {
      "command": "vitest --run --passWithNoTests",
      "files": ["src/**/*"],
      "output": []
    },
    "postinstall": {
      "command": "is-ci || husky install"
    },
    "precommit": {
      "command": "lint-staged"
    }
  }
}
