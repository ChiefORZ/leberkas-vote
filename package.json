{
  "name": "@chieforz/leberkas-vote",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "ChiefORZ <seb.schaffernak@gmail.com>",
  "scripts": {
    "prebuild": "pnpm run generate",
    "build": "pnpm run migrate:deploy && nx exec -- next build",
    "dev": "next",
    "generate": "run-s generate:prisma generate:nexus",
    "generate:nexus": "nx exec -- tsx --tsconfig nexus.tsconfig.json ./src/pages/api",
    "generate:prisma": "prisma generate",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "migrate:dev": "is-ci || prisma migrate dev",
    "migrate:deploy": "is-ci || prisma migrate deploy",
    "migrate:status": "is-ci || prisma migrate status",
    "prestart": "prisma generate",
    "start": "next start",
    "test": "npm-run-all test:*",
    "test:knip": "knip",
    "test:unit": "vitest --run --passWithNoTests",
    "postinstall": "is-ci || husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "prettier": "@chieforz/prettier-config",
  "eslintConfig": {
    "extends": "@chieforz/eslint-config-tslint-react"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@as-integrations/next": "^2.0.2",
    "@aws-sdk/abort-controller": "^3.370.0",
    "@aws-sdk/client-s3": "^3.433.0",
    "@aws-sdk/signature-v4-crt": "^3.433.0",
    "@formkit/auto-animate": "^1.0.0-pre-alpha.3",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@magic-sdk/admin": "^2.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.4.2",
    "@splitbee/web": "^0.3.0",
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.16",
    "aws-crt": "^1.18.1",
    "aws-sdk": "^2.1478.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-scalars": "^1.22.4",
    "magic-sdk": "^21.1.1",
    "micro-cors": "^0.1.1",
    "next": "^13.4.7",
    "next-auth": "^4.22.1",
    "next-s3-upload": "^0.3.3",
    "nexus": "^1.3.0",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.69.4",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@chieforz/eslint-config-tslint-react": "^1.1.9",
    "@chieforz/prettier-config": "^1.1.9",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/micro-cors": "^0.1.4",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.1.0",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^22.1.0",
    "knip": "^2.37.0",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "nx": "^16.10.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.4.2",
    "resize-observer-polyfill": "^1.5.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=18.17"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.next"
        ]
      },
      "generate:nexus": {
        "outputs": [
          "{projectRoot}/generated"
        ]
      }
    }
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "6.2.0",
      "@typescript-eslint/parser": "6.2.0",
      "@typescript-eslint/utils": "6.2.0"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
