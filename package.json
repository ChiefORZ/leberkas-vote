{
  "name": "@chieforz/leberkas-vote",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "ChiefORZ <seb.schaffernak@gmail.com>",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "format:check": "wireit",
    "format:write": "wireit",
    "generate": "wireit",
    "generate:nexus": "wireit",
    "generate:prisma": "wireit",
    "postinstall": "wireit",
    "lint": "wireit",
    "migrate:deploy": "wireit",
    "migrate:dev": "wireit",
    "migrate:status": "wireit",
    "precommit": "wireit",
    "start": "wireit",
    "test": "wireit",
    "test:knip": "wireit",
    "test:unit": "wireit"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "biome check --apply"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@as-integrations/next": "^3.1.0",
    "@aws-sdk/abort-controller": "^3.374.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/signature-v4-crt": "^3.635.0",
    "@formkit/auto-animate": "^1.0.0-pre-alpha.3",
    "@headlessui/react": "^2.1.3",
    "@heroicons/react": "^2.1.5",
    "@magic-sdk/admin": "^2.4.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.18.0",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.20",
    "aws-crt": "^1.21.8",
    "aws-sdk": "^2.1682.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "graphql-scalars": "^1.23.0",
    "magic-sdk": "^28.5.0",
    "micro-cors": "^0.1.1",
    "next": "^13.4.7",
    "next-auth": "^4.22.1",
    "next-s3-upload": "^0.3.4",
    "nexus": "^1.3.0",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.77.8",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@chieforz/biome-config": "^1.3.0",
    "@chieforz/eslint-config-tslint-react": "^1.3.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/micro-cors": "^0.1.5",
    "@types/node": "^20.16.1",
    "@types/react": "^18.3.4",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.5",
    "encoding": "^0.1.13",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "is-ci": "^3.0.1",
    "jsdom": "^24.1.3",
    "knip": "^5.27.4",
    "lint-staged": "^15.2.9",
    "npm-run-all2": "^6.2.2",
    "prisma": "^5.18.0",
    "resize-observer-polyfill": "^1.5.1",
    "tsx": "^4.18.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0",
    "webpack": "^5.94.0",
    "wireit": "^0.14.8"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": "^20"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "7.18.0",
      "@typescript-eslint/parser": "7.18.0",
      "@typescript-eslint/utils": "7.18.0"
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "wireit": {
    "build": {
      "command": "next build",
      "dependencies": [
        "generate"
      ],
      "files": [
        "prisma/schema.prisma",
        "src/**/*",
        "tsconfig.json"
      ],
      "output": [
        ".next/**"
      ]
    },
    "dev": {
      "command": "next",
      "dependencies": [
        "generate"
      ],
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "format:check": {
      "command": "biome check ./src",
      "files": [
        "src/**/*.{js,jsx,ts,tsx}"
      ],
      "output": []
    },
    "format:write": {
      "command": "biome check --apply ./src"
    },
    "generate": {
      "command": "run-s generate:prisma generate:nexus"
    },
    "generate:nexus": {
      "command": "tsx --tsconfig nexus.tsconfig.json ./src/pages/api"
    },
    "generate:prisma": {
      "command": "prisma generate"
    },
    "lint": {
      "command": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "migrate:dev": {
      "command": "is-ci || prisma migrate dev",
      "files": [
        "prisma/migrations/**/*",
        "prisma/schema.prisma"
      ],
      "output": [
        "node_modules/.prisma/client/**"
      ]
    },
    "migrate:deploy": {
      "command": "is-ci || prisma migrate deploy",
      "files": [
        "prisma/migrations/**/*",
        "prisma/schema.prisma"
      ],
      "output": [
        "node_modules/.prisma/client/**"
      ]
    },
    "migrate:status": {
      "command": "is-ci || prisma migrate status",
      "files": [
        "prisma/migrations/**/*",
        "prisma/schema.prisma"
      ],
      "output": []
    },
    "start": {
      "command": "next start",
      "dependencies": [
        "generate:prisma",
        "migrate:deploy"
      ],
      "files": [
        ".next/**"
      ],
      "output": []
    },
    "test": {
      "command": "npm-run-all test:*",
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "test:knip": {
      "command": "knip"
    },
    "test:unit": {
      "command": "vitest --run --passWithNoTests",
      "files": [
        "src/**/*"
      ],
      "output": []
    },
    "postinstall": {
      "command": "is-ci || husky install"
    },
    "precommit": {
      "command": "lint-staged"
    }
  }
}
